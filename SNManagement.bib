@article{ ISI:000390743400022,
Author = {Hung, Chia-Liang},
Title = {{Social networks, technology ties, and gatekeeper functionality:
   Implications for the performance management of R\&D projects}},
Journal = {{RESEARCH POLICY}},
Year = {{2017}},
Volume = {{46}},
